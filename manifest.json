{
  "name": "ADHD Copilot",
  "description": "",
  "version": "0.0.1",
  "manifest_version": 3,
  "action": {
    "default_popup": "pages/popup.html",
    "default_icon": "images/icon.png"
  },
  "content_scripts": [
    {
      "js": [
        "scripts/workTimeGuard.js",
        "scripts/pageTimeTracker.js"
      ],
      "css": [],
      "matches": [
        "https://*.bilibili.com/*",
        "https://*.ruanyifeng.com/*",
        "https://*.v2ex.com/*",
        "https://*.zhihu.com/*"
      ]
    },
    {
      "js": [
        "scripts/163.js"
      ],
      "css": [
        "styles/163.css"
      ],
      "matches": [
        "https://*.163.com/*"
      ]
    },
    {
      "js": [
        "scripts/baidu.js"
      ],
      "css": [
        "styles/baidu.css"
      ],
      "matches": [
        "https://baidu.com/*",
        "https://*.baidu.com/*"
      ]
    },
    {
      "js": [
        "scripts/cnki.js"
      ],
      "css": [
        "styles/cnki.css"
      ],
      "matches": [
        "https://*.cnki.net/*"
      ]
    },
    {
      "js": [
        "scripts/sohu.js"
      ],
      "css": [
        "styles/sohu.css"
      ],
      "matches": [
        "https://*.sohu.com/*"
      ]
    },
    {
      "js": [
        "scripts/v2ex.js"
      ],
      "css": [
        "styles/v2ex.css"
      ],
      "matches": [
        "https://*.v2ex.com/*"
      ]
    },
    {
      "js": [
        "scripts/coolshell.js"
      ],
      "css": [
        "styles/coolshell.css"
      ],
      "matches": [
        "https://coolshell.cn/*"
      ]
    },
    {
      "js": [
        "scripts/yinwang.js"
      ],
      "css": [
        "styles/yinwang.css"
      ],
      "matches": [
        "https://*.yinwang.org/*"
      ]
    },
    {
      "js": [
        "scripts/zhihu.js"
      ],
      "css": [
        "styles/zhihu.css"
      ],
      "matches": [
        "https://*.zhihu.com/*"
      ]
    },
    {
      "js": [],
      "css": [
        "styles/pbc.css"
      ],
      "matches": [
        "http://www.pbc.gov.cn/goutongjiaoliu/*"
      ]
    },
    {
      "js": [],
      "css": [
        "styles/monkeytype.css"
      ],
      "matches": [
        "https://monkeytype.com/"
      ]
    }
  ],
  "permissions": [
    "storage"
  ]
}